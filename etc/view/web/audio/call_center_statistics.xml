<?xml version="1.0" encoding="UTF-8"?>
<viewport>
    <!--面包屑-->
    <crumbs>
        <crumb text="电话录音报表" url="javascript:void(0);"/>
        <crumb text="Call Center跟踪" currentPage="true"/>
    </crumbs>

    <customgridpanel url="/callCenterStatistics.json" pageSize="15" title="Call Center统计数据" id="grid1" width="1200" height="350" loadUrl="false">
        <column field="centerName" desc="所属中心">
            <formatter>
                <![CDATA[
                    return '<a style="color:blue;text-decoration: underline;" title="查看该中心CallCenter详情" href="/web/audio/center_callcenter_detail?centerId='+options.rowId+'">'+cellvalue+'</a>'
                ]]>
            </formatter>
        </column>
        <column field="totalNum" desc="名单总数"/>
        <column field="noContactNum" desc="未联系的名单数"/>
        <column field="waitNum" desc="待确认的名单数"/>
        <column field="badNum" desc="已废弃的名单数"/>
        <column field="inviteNum" desc="已邀约的名单数"/>
        <column field="sureNum" desc="确认签到的名单数"/>
    </customgridpanel>
</viewport>