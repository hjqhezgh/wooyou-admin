<?xml version="1.0" encoding="UTF-8"?>
<viewport window="true">
    <customformpanel id="form" title="添加接触记录" saveUrl="/web/consumerContactsLog/save.json" load="false" inwindow="true">
        <element field="contactType"  type="localSelect" validate="notNull" localData="[{desc:'电话', value:'1'},{desc:'当面沟通', value:'2'}]" desc="联系方式"/>
        <element field="consumer_id" type="hidden" validate="" desc="客户Id" defaultValue="$consumerId"/>
        <element field="note" type="textarea" validate="notNull" desc="接触记录"/>
    </customformpanel>
</viewport>