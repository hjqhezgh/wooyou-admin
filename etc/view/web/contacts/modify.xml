<?xml version="1.0" encoding="UTF-8"?>
<viewport window="true">
    <customformpanel id="form" title="修改联系人" saveUrl="/contacts/save.json" load="true" inwindow="true" loadUrl="/web/contacts/contactsLoad.json">
        <element field="id" type="hidden" validate="" desc="主键"/>
        <element field="name" type="text" validate="" desc="联系人名字"/>
        <element field="phone" type="text" validate="notNull" desc="联系电话"/>
        <element field="is_default"  type="localSelect" localData="[{desc:'是', value:'1'},{desc:'否', value:'2'}]" desc="是否默认联系人"/>
    </customformpanel>
</viewport>